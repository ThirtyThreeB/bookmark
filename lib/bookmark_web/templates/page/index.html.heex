<section class="row">
  <article class="column">
    <h2>Bookmarks</h2>
    <table>
      <tr>
        <th>ID</th>
        <th>url</th>
        <th>code</th>
      </tr>
      <%= for b <- @bookmark do %>
        <tr>
          <th><%= b.id %></th>
          <th><%= b.url %></th>
          <th><%= link("#{b.code}", to: URI.parse("#{b.url}")) %></th>
        </tr>
      <% end %>
    </table>
  </article>
</section>
